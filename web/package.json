{
  "name": "cirodown-web",
  "version": "0.0.1",
  "description": "cirodown web",
  "main": "app.js",
  "scripts": {
    "build": "next build",
    "build-dev": "NODE_ENV_OVERRIDE=development npm run build",
    "dev": "nodemon --unhandled-rejections=strict ./app.js",
    "dev2": "concurrently --kill-others 'npm run front --' 'npm run back --'",
    "postinstall": "cd .. && npm link cirodown",
    "start": "node --unhandled-rejections=strict ./app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gothinkster/productionready-node-api.git"
  },
  "license": "AGPL",
  "dependencies": {
    "body-parser": "1.15.0",
    "cirodown": "..",
    "cors": "2.7.1",
    "errorhandler": "1.4.3",
    "express": "4.13.4",
    "express-jwt": "3.3.0",
    "express-session": "1.13.0",
    "jsonwebtoken": "7.1.9",
    "method-override": "2.3.5",
    "methods": "1.1.2",
    "morgan": "1.7.0",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "pg": "8.5.1",
    "request": "2.69.0",
    "sequelize": "6.5.1",
    "slug": "0.9.1",
    "underscore": "1.8.3",
    "@emotion/core": "10.0.28",
